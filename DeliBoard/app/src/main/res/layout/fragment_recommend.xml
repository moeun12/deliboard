<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="Recommend.Recommend">

    <CheckBox
        android:id="@+id/checkbox_easy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="초급"
        tools:layout_editor_absoluteX="600dp"
        tools:layout_editor_absoluteY="310dp"
        tools:ignore="MissingConstraints" />

    <CheckBox
        android:id="@+id/checkbox_medium"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/checkbox_easy"
        android:text="중급"
        tools:layout_editor_absoluteX="400dp"
        tools:layout_editor_absoluteY="295dp"
        tools:ignore="MissingConstraints" />

    <CheckBox
        android:id="@+id/checkbox_hard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/checkbox_medium"
        android:text="고급"
        tools:layout_editor_absoluteX="248dp"
        tools:layout_editor_absoluteY="295dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>